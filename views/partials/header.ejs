<header class="site-header">
        <h1>
        <span class="siteName">
            <a href="/" title="Return to home page">CSE Motors</a>
        </span>
    </h1>
 <% if(locals.loggedin == 1) { %>
    <div id="tools" class="header-tools">
        <a title="Account page" href="/account" class="tool-link account-link">Welcome <%- locals.accountData.account_firstname %></a>
        <a title="Log out" href="/account/logout" class="tool-link logout-link">Logout</a>
    </div>
    <% } else { %>
        <div id="tools" class="header-tools">
            <a title="Click to log in" href="/account/login" class="tool-link login-link">My Account</a>
        </div>
        <% } %>
</header>



